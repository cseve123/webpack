{
    "presets": [
        "@babel/preset-react"
    ],
    // babel-transform-runtime 让polyfill不污染全局
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true,
                "useESModules": false
            }
        ]
    ]
}